&lt;?xml version="1.0" encoding="UTF-8"?&gt;
&lt;payload&gt;
    &lt;station&gt;
        &lt;callsign&gt;{{station.callsign}}&lt;/callsign&gt;
    &lt;/station&gt;
    &lt;purchase&gt;
        &lt;isApproved&gt;{{purchase.is_approved}}&lt;/isApproved&gt;
    &lt;/purchase&gt;
    &lt;contract&gt;
        &lt;amount&gt;{{contract.amount}}&lt;/amount&gt;
    &lt;/contract&gt;
    &lt;advertiser&gt;
        &lt;name&gt;{{advertiser.name}}&lt;/name&gt;
        &lt;contact&gt;
            &lt;name&gt;{{advertiser.contact.name}}&lt;/name&gt;
            &lt;address&gt;&lt;![CDATA[{{advertiser.contact.address}}]]&gt;&lt;/address&gt;
            &lt;phone&gt;{{advertiser.contact.phone}}&lt;/phone&gt;
        &lt;/contact&gt;
    &lt;/advertiser&gt;
    &lt;advertisement&gt;
        &lt;subject&gt;{{advertisement.subject}}&lt;/subject&gt;
        &lt;isByCandidate&gt;{{advertisement.is_by_candidate}}&lt;/isByCandidate&gt;
    &lt;/advertisement&gt;
    &lt;subject&gt;
        &lt;fecId&gt;{{subject.fec_id}}&lt;/fecId&gt;
        &lt;name&gt;{{subject.name}}&lt;/name&gt;
        &lt;officeSought&gt;{{subject.office_sought}}&lt;/officeSought&gt;
    &lt;/subject&gt;
    &lt;committee&gt;
        &lt;name&gt;{{committee.name}}&lt;/name&gt;
        &lt;treasurer&gt;{{committee.treasurer}}&lt;/treasurer&gt;
    &lt;/committee&gt;
